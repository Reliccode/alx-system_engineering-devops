sudo docker exec -it b2ee240c7d6c /bin/bash
cd /etc/apache2/sites-available
/etc/apache2/sites-available# ls
/etc/apache2/sites-available# nano 000-default.conf
/etc/apache2/sites-available# cd /var/www/html
/var/www/html# ls
service apache2 restart
nano /etc/apache2/apache2.conf #Added ServerName -myserverip
apache2ctl configtest
service apache2 restart
cd /var/www/html
/var/www/html# ls
service apache2 restart
exit
